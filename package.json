{
  "name": "abc-react",
  "version": "1.0.21",
  "description": "It is abc testing platform.",
  "author": "yaser2us",
  "license": "MIT",
  "repository": "yaser2us/abc-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle --no-compress --jsx React.createElement --jsxFragment React.Fragment --format modern,cjs",
    "yoo": "microbundle --no-compress --format modern,cjs",
    "start": "microbundle watch --no-compress --format modern,cjs",
    "love": "cd example && npm start",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.8.6 || ^17.0.0 || ^17.0.1 || ^17.0.2 || ^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "gh-pages": "^2.2.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^5.0.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@growthbook/growthbook-react": "^0.18.0",
    "url-parse": "^1.5.10",
    "lodash": "^4.17.20"
  },
  "microbundle": {
    "entries": "src/index.js",
    "globals": {
      "react": "React"
    }
  }
}
